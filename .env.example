# Runtime
GOOGLE_API_KEY=not_available
ADK_MODEL=gemini-2.5-flash-lite
ADK_APP_NAME=autonomous-secops-platform
ADK_USER_ID=local-user
ADK_SESSION_ID=local-session
ADK_PROMPT=Run a quick health check and summarize any risks.

# Session storage
ADK_SESSION_DB_URL=not_available
ADK_SESSION_DB_PATH=./data/adk_sessions.db

# Memory service
ADK_MEMORY_BACKEND=in_memory
ADK_PRELOAD_MEMORY=false
GOOGLE_CLOUD_PROJECT=not_available
GOOGLE_CLOUD_LOCATION=not_available
ADK_AGENT_ENGINE_ID=not_available

# Metrics backend (Prometheus/VictoriaMetrics)
METRICS_BACKEND_URL=not_available
PROMETHEUS_URL=not_available
VICTORIAMETRICS_URL=not_available
METRICS_BEARER_TOKEN=not_available

# Optional query overrides
METRIC_QUERY_CPU_USAGE_PERCENT=not_available
METRIC_QUERY_MEMORY_USAGE_PERCENT=not_available
METRIC_QUERY_DISK_USAGE_PERCENT=not_available
METRIC_QUERY_NETWORK_IO_MBPS=not_available
METRIC_QUERY_REQUEST_P95_MS=not_available
METRIC_QUERY_ERROR_RATE_PERCENT=not_available
